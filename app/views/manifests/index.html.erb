<h1> Grand Manifest Lists </h1>
<h3> ============================ </h3>

<div class = "center list-group">

  <% @manifests.each do |mfst| %>
    <li class="list-group-item">
      <%= link_to "#{mfst.title}", manifest_path(mfst.id) %> -->  <%= link_to "Delete!", manifest_path(mfst.id), method: :delete %><br/>
    </li>
  <% end %>
</div>

<br/><br/><br/>

<div class="btn-group">
  <%= link_to "Create!", new_manifest_path, class: "btn btn-large btn-primary" %>
</div>

